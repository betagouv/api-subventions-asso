{
    "name": "api",
    "version": "0.39.0",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "18.17",
        "npm": "9.6.7"
    },
    "scripts": {
        "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
        "start": "node ./build/src/index.js",
        "cli": "ts-node src/cli.ts",
        "generate:docs": "tsoa spec",
        "generate:routes": "tsoa routes",
        "generate": "npm run generate:routes && npm run generate:docs",
        "prebuild": "npm run generate",
        "build": "tsc --project tsconfig.build.json",
        "test": "npm run test:unit && npm run test:integ",
        "test:unit": "NODE_OPTIONS=--max-old-space-size=8192 jest -c jest.config.unit.ts --runInBand",
        "test:integ": "NODE_OPTIONS=--max-old-space-size=8192 jest -c jest.config.integ.ts --runInBand",
        "test:cov": "npm run test:unit -- --coverage",
        "migration:new": "migrate-mongo create",
        "migration:status": "migrate-mongo status",
        "migration:apply": "migrate-mongo up",
        "lint": "eslint --fix --ext .js,.ts",
        "lint:all": "eslint --fix --ext .js,.ts ./{migrations,src,tests,tools}/**/*.{js,ts} ./{migrations,src,tests,tools}/*.{js,ts}",
        "prettier": "prettier --write",
        "prettier:all": "prettier --write ./{migrations,src,tests,tools}/**/*.{js,ts}",
        "lint-format": "lint-staged"
    },
    "lint-staged": {
        "./{migrations,src,tests,tools}/**/*.{js,ts}": [
            "npx prettier --write --loglevel warn",
            "npx eslint --fix --quiet"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/betagouv/api-subventions-asso.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/betagouv/api-subventions-asso/issues"
    },
    "homepage": "https://github.com/betagouv/api-subventions-asso#readme",
    "dependencies": {
        "@getbrevo/brevo": "^1.0.1",
        "@sentry/node": "^7.54.0",
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/decompress": "^4.2.4",
        "@types/express": "^4.17.13",
        "@types/express-winston": "^4.0.0",
        "@types/lodash": "^4.14.191",
        "@types/moment": "^2.13.0",
        "@types/nodemailer": "^6.4.4",
        "@types/passport": "^1.0.7",
        "@types/passport-jwt": "^3.0.6",
        "@types/passport-local": "^1.0.34",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/unzipper": "^0.10.7",
        "@types/winston": "^2.4.4",
        "axios": "^1.4.0",
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "croner": "^6.0.3",
        "decompress": "^4.2.1",
        "dedent": "^0.7.0",
        "dotenv": "^14.3.2",
        "dto": "^0.39.0",
        "express": "^4.17.2",
        "express-winston": "^4.2.0",
        "iconv-lite": "^0.6.3",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "migrate-mongo": "^8.2.3",
        "moment": "^2.29.4",
        "mongodb": "^4.3.0",
        "node-xlsx": "^0.23",
        "nodemailer": "^6.7.2",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "qs": "^6.11.0",
        "rand-token": "^1.0.1",
        "reflect-metadata": "^0.1.13",
        "swagger-ui-express": "^4.3.0",
        "toad-scheduler": "^2.2.0",
        "tqdm": "^2.0.3",
        "tsoa": "^5.0.0",
        "unzip": "^0.1.11",
        "unzipper": "^0.10.14",
        "winston": "^3.5.1",
        "winston-mongodb": "^5.1.1",
        "xss": "^1.0.14"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^2.2.0",
        "@types/jest": "^27.4.0",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.9.0",
        "@typescript-eslint/parser": "^5.9.0",
        "concurrently": "^7.0.0",
        "crypto": "^1.0.1",
        "eslint": "^8.6.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^27.2.1",
        "jest": "^28.0.0",
        "lint-staged": "^13.2.2",
        "nodemon": "^3.0.1",
        "supertest": "^6.2.2",
        "ts-jest": "^28.0.0",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    }
}
