{
    "name": "api",
    "version": "0.46.3",
    "description": "",
    "main": "index.js",
    "engines": {
        "node": "18.17",
        "npm": "9.6.7"
    },
    "scripts": {
        "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
        "start": "node ./build/src/index.js",
        "cli": "ts-node src/cli.ts",
        "generate:docs": "tsoa spec",
        "generate:routes": "tsoa routes",
        "generate": "npm run generate:routes && npm run generate:docs",
        "prebuild": "npm run generate",
        "build": "tsc --project tsconfig.build.json",
        "test": "npm run test:unit && npm run test:integ",
        "test:unit": "NODE_OPTIONS=--max-old-space-size=8192 jest -c jest.config.unit.ts --runInBand",
        "test:integ": "NODE_OPTIONS=--max-old-space-size=8192 jest -c jest.config.integ.ts --runInBand",
        "test:cov": "npm run test:unit -- --coverage",
        "migration:new": "migrate-mongo create",
        "migration:status": "migrate-mongo status",
        "migration:apply": "migrate-mongo up",
        "lint": "eslint --fix --ext .js,.ts",
        "lint:all": "eslint --fix --ext .js,.ts ./{migrations,src,tests,tools}/**/*.{js,ts} ./{migrations,src,tests,tools}/*.{js,ts}",
        "prettier": "prettier --write",
        "prettier:all": "prettier --write ./{migrations,src,tests,tools}/**/*.{js,ts}",
        "lint-format": "lint-staged"
    },
    "lint-staged": {
        "./{migrations,src,tests,tools}/**/*.{js,ts}": [
            "npx prettier --write --loglevel warn",
            "npx eslint --fix --quiet"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/betagouv/api-subventions-asso.git"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/betagouv/api-subventions-asso/issues"
    },
    "homepage": "https://github.com/betagouv/api-subventions-asso#readme",
    "dependencies": {
        "@getbrevo/brevo": "^1.0.1",
        "@sentry/node": "^7.54.0",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.17",
        "@types/decompress": "^4.2.7",
        "@types/express": "^4.17.21",
        "@types/lodash": "^4.14.202",
        "@types/nodemailer": "^6.4.14",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^3.0.13",
        "@types/passport-local": "^1.0.38",
        "@types/swagger-ui-express": "^4.1.6",
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "croner": "^6.0.7",
        "csv-parse": "^5.5.3",
        "decompress": "^4.2.1",
        "dedent": "^0.7.0",
        "dotenv": "^14.3.2",
        "dto": "^0.46.0",
        "express": "^4.18.2",
        "express-winston": "^4.2.0",
        "fast-xml-parser": "^4.3.5",
        "fuse.js": "^7.0.0",
        "iconv-lite": "^0.6.3",
        "jest-environment-jsdom": "^29.7.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "migrate-mongo": "^8.2.3",
        "moment": "^2.29.4",
        "mongodb": "^4.17.2",
        "node-xlsx": "^0.23.0",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "qs": "^6.11.2",
        "rand-token": "^1.0.1",
        "reflect-metadata": "^0.1.14",
        "swagger-ui-express": "^4.6.3",
        "toad-scheduler": "^2.3.0",
        "tqdm": "^2.0.3",
        "tsoa": "^5.1.1",
        "winston": "^3.11.0",
        "winston-mongodb": "^5.1.1",
        "xss": "^1.0.14"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^2.2.2",
        "@types/jest": "^27.5.2",
        "@types/node": "^20.10.4",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "concurrently": "^7.6.0",
        "eslint": "^8.55.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.6.0",
        "jest": "^28.1.3",
        "lint-staged": "^13.3.0",
        "nodemon": "^3.0.2",
        "supertest": "^6.3.3",
        "ts-jest": "^28.0.8",
        "ts-node": "^10.9.2",
        "typescript": "^4.5.4"
    }
}
