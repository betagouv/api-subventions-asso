{
  "name": "api",
  "version": "0.11.3",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "node ./build/src/index.js",
    "cli": "ts-node src/cli.ts",
    "generate:docs": "tsoa swagger",
    "generate:routes": "tsoa routes",
    "generate": "npm run generate:routes && npm run generate:docs",
    "prebuild": "npm run generate",
    "build": "tsc --project ./",
    "prelint": "npm run generate",
    "lint": "eslint . --ext .ts",
    "test": "npm run test:unit && npm run test:integ",
    "test:unit": "NODE_OPTIONS=--max-old-space-size=8192 jest -c jest.config.unit.ts --runInBand",
    "test:integ": "NODE_OPTIONS=--max-old-space-size=8192 jest -c jest.config.integ.ts --runInBand",
    "test:cov": "npm run test:unit -- --coverage",
    "migration:new": "migrate-mongo create",
    "migration:status": "migrate-mongo status",
    "migration:apply": "migrate-mongo up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/betagouv/api-subventions-asso.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/betagouv/api-subventions-asso/issues"
  },
  "homepage": "https://github.com/betagouv/api-subventions-asso#readme",
  "dependencies": {
    "@api-subventions-asso/dto": "../dto",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongo": "^8.2.3",
    "mongodb": "^4.3.0",
    "node-xlsx": "^0.21.0",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "qs": "^6.11.0",
    "rand-token": "^1.0.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.4.0",
    "tsoa": "^3.2.1",
    "winston": "^3.5.1",
    "winston-mongodb": "^5.0.7"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-winston": "^4.0.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.7",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "crypto": "^1.0.1",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
