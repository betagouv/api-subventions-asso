{
    "compilerOptions": {
        "target": "es2022",
        "module": "esnext",
        "outDir": "build",
        "moduleResolution": "node10",
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "strict": true,
        "declaration": true
    },
    "exclude": ["node_modules", "build"]
}
