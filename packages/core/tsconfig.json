  "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "rootDir": "./",
      "outDir": "./build",
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true,
      "strict": true,
      "declaration": true
  },
  "exclude": ["node_modules"]
}
