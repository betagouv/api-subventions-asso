<div class="fr-col-12 fr-col-md-4">
    <div class="fr-card fr-enlarge-link">
        <div class="fr-card__body">
            <% if (helper.hasValue(association.denomination)) { %>
                <h2 class="fr-card__title"><a href="#" class="fr-card__link"><%- helper.getValue(association.denomination) -%></a></h2>
            <% } %>
            <div class="fr-grid-row fr-grid-row--left fr-grid-row--gutters">
                <div class="fr-col fr-col-lg-12 fr-card__desc">
                    <% if (helper.hasValue(etablissement.nic) && helper.hasValue(association.nic_siege) && helper.getValue(etablissement.nic) == helper.getValue(association.nic_siege) ) { %>
                        <p>Siège de l'association</p>
                    <% } else { %>
                        <p>Établissement secondaire</p>
                    <% } %>
                            
                    <% if (helper.hasValue(etablissement.siret)) { %>
                        <p>SIRET : <%- helper.getValue(etablissement.siret) -%> </p>
                    <% } %>
                    <% if (helper.hasValue(etablissement.adresse)) { %>
                        <p><%- helper.adresse.toString(etablissement.adresse) -%> </p>
                    <% } %>

                </div>
            </div>

        </div>
    </div>
</div>