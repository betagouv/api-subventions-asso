{
    "name": "front",
    "version": "0.31.4",
    "description": "> TODO: description",
    "author": "Victor Serain <victor.serain@epitech.eu>",
    "homepage": "https://github.com/betagouv/api-subventions-asso/tree/main/packages/front#readme",
    "license": "MIT",
    "main": "build/index.js",
    "svelte": "build/svelte-index.js",
    "directories": {
        "lib": "lib",
        "test": "__tests__"
    },
    "files": [
        "lib"
    ],
    "engines": {
        "node": "16",
        "npm": "8"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/betagouv/datasubvention.git"
    },
    "scripts": {
        "dev": "nodemon",
        "start": "node ./build/src/index.js",
        "prebuild": "rm -rf ./build/* && webpack && webpack --config ./webpack.svelte.config.js",
        "build": "tsc --project tsconfig.build.json",
        "postbuild": "npm run copy-files",
        "copy-files": "cp -r views build && cp -r static build && mkdir build/node_modules && cp -r node_modules/@gouvfr build/node_modules/@gouvfr",
        "test": "npm run test:unit",
        "test:unit": "jest -c jest.config.unit.js --runInBand",
        "test:cov": "npm run test:unit -- --coverage",
        "dev:svelte": "webpack --config webpack.svelte.config.js",
        "lint": "eslint --fix --ext .js,.ts,.svelte",
        "lint:all": "eslint --fix --ext .js,.ts,.svelte ./{src,svelte,views}/**/*.{js,ts,svelte} ./{src,svelte,views}/*.{js,ts,svelte}",
        "prettier": "prettier --write",
        "prettier:all": "prettier --write ./{src,svelte,views}/**/*.{js,ts,svelte}",
        "lint-format": "lint-staged"
    },
    "lint-staged": {
        "./{src,svelte,views}/**/*.{js,ts,svelte}": [
            "npx prettier --write --loglevel warn",
            "npx eslint --fix --quiet"
        ]
    },
    "bugs": {
        "url": "https://github.com/betagouv/datasubvention/issues"
    },
    "dependencies": {
        "@api-subventions-asso/dto": "../dto",
        "@gouvfr/dsfr": "*",
        "@hotwired/stimulus": "^3.0.1",
        "@hotwired/stimulus-webpack-helpers": "^1.0.1",
        "@hotwired/turbo": "^7.1.0",
        "axios": "^0.26.0",
        "body-parser": "^1.19.2",
        "chart.js": "^4.0.1",
        "connect-mongo": "^4.6.0",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.0",
        "ejs": "^3.1.7",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "express-winston": "^4.2.0",
        "lodash": "^4.17.21",
        "reflect-metadata": "^0.1.13",
        "string-similarity": "^4.0.4",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.4.0",
        "winston": "^3.8.0",
        "winston-mongodb": "^5.0.7"
    },
    "devDependencies": {
        "@babel/core": "^7.20.2",
        "@babel/plugin-transform-modules-commonjs": "^7.19.6",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^29.2.3",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.9.0",
        "babel-jest": "^29.3.1",
        "concurrently": "^7.0.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.18.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-svelte3": "^4.0.0",
        "jest": "^29.1.2",
        "jest-environment-jsdom": "^29.3.1",
        "lint-staged": "^13.2.2",
        "mini-css-extract-plugin": "^2.6.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.7.0",
        "rollup": "^2.75.6",
        "serve": "^14.1.2",
        "svelte": "^3.49.0",
        "svelte-jester": "^2.3.2",
        "svelte-loader": "^3.1.3",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.3.0",
        "typescript": "^4.5.4",
        "webpack": "^5.76.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.9.2"
    }
}
