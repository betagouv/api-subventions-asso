<div class="fr-table">
    <table class="custom-table-center-text">
        <thead>
            <tr>
                <th scope="col">Provider</th>
                <th scope="col">Domaine fonctionnel</th>
                <th scope="col">Activitée</th>
                <th scope="col">Centre financier</th>
                <th scope="col">N°Tier</th>
                <th scope="col">N°EJ</th>
                <th scope="col">N°Demande de paiements</th>
                <th scope="col">Montants</th>
                <th scope="col">Date</th>
                <th scope="col">Plus</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i = 0; i < versements.length; i++ ) { %>
                <% const versement = versements[i];%> 
                <tr>
                    <td><%-helper.returnValueOrHyphen(helper.getProvider(versement.amount))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(versement.domaineFonctionnel))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(versement.activitee))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(versement.centreFinancier))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(versement.numeroTier))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(versement.ej))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(versement.numeroDemandePayment))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.currency.addSpaces(helper.getValue(versement.amount)))%> €</td>
                    <td><%-helper.returnValueOrHyphen(helper.date.toProviderValueString(versement.dateOperation))%></td>
                    <td>
                        <% const siret = helper.getValue(versement.siret); %>
                        <% if (currentSiret != siret) { %> 
                            <a href="/etablissement/<%-siret%>">Voir l'établissement</a>
                        <% } else { %>
                            <%-siret%>
                        <% } %>
                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>