<div class="fr-table">
    <table class="custom-table-center-text">
        <thead>
            <tr>
                <th scope="col">Statut</th>
                <th scope="col">Service instructeur</th>
                <th scope="col">Dispositif</th>
                <th scope="col">N°EJ</th>
                <th scope="col">Année de la demande</th>
                <th scope="col">Date de la commision</th>
                <th scope="col">Pluriannuel</th>
                <th scope="col">Montant accordé</th>
                <th scope="col">Source</th>
                <th scope="col">Plus d'informations</th>
            </tr>
        </thead>
        <tbody>
            <% for(let i = 0; i < subventions.demandes[year].length; i++ ) { %>
                <% const demande = subventions.demandes[year][i];%> 
                <tr>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(demande.status))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(demande.service_instructeur))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(demande.dispositif))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(demande.ej))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(demande.annee_demande))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.date.toProviderValueString(demande.date_commision))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.getValue(demande.pluriannualite))%></td>
                    <td><%-helper.returnValueOrHyphen(helper.currency.addSpaces(helper.getValue(demande.montants.accorde)))%> €</td>
                    <td><%-helper.returnValueOrHyphen(helper.getProvider(demande.service_instructeur) )%></td>
                    <td>
                        <a download="demande_subvention.json" href="data:application/json;base64,<%- helper.toJSONFile(demande) %>">
                            <button class="fr-btn fr-fi-file-download-line" title="Télécharger les données sur la demande de subvention">
                                Télécharger les données sur la demande de subvention
                            </button>
                        </a>

                    </td>
                </tr>
            <% } %>
        </tbody>
    </table>
</div>
